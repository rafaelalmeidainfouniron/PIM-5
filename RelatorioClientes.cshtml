



@{
    Layout = "_LayoutTabela.cshtml";
    var conexão = WebMatrix.Data.Database.Open("SCS");
    var cliente = conexão.Query("select * from CLIENTE");
    conexão.Close();
}




<div class="col-lg-10">
    <center>
        <h1> Relatorio de Clientes </h1> 
    </center>
   

 
    <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
        <thead>
            <tr>
                <th id="nome">Data do Cadastro</th>
                <th id="cpf">CPF</th>
                <th id="cpf">Nome</th>

                <th id="acoes">Cidade</th>

            </tr>
        </thead>
        @foreach (var c in cliente)
        {
            <tr>
                <td>@c.DataEntrada</td>
                <td>@c.cpf</td>
                <td>@c.nome</td>
                <td>@c.cidade</td>

               
            </tr>




        }
    </table>
    <br />
    
  
    
       

        <a href="#"><button type="button" class="btn btn-default "> Imprimir</button></a>
    </div>

    
    <br />
